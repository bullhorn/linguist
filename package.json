{
  "name": "linguist",
  "author": "Brian Kimball <bvkimball@gmail.com>",
  "version": "0.1.0",
  "description": "A universal localization managment tool for web projects",
  "bin": {
    "linguist": "lib/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "compile:watch": "babel -d lib/ src/ --watch",
    "prepublish": "npm run compile",
    "test": "ava"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvkimball/linguist"
  },
  "keywords": [
    "i18n",
    "l10n",
    "Localization"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bvkimball/linguist/issues"
  },
  "homepage": "https://github.com/bvkimball/linguist#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "clear": "0.0.1",
    "commander": "^2.9.0",
    "fs-readline": "^2.1.0",
    "handlebars": "^4.0.5",
    "inquirer": "^1.1.2",
    "lodash": "4.3.0",
    "log-update": "^1.0.2",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.6.0",
    "passport": "^0.3.2",
    "rc": "^1.1.6",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^14.1.0",
    "jshint-summary": "^0.4.0"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
