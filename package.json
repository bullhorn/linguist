{
  "name": "linguist",
  "author": "Brian Kimball <bvkimball@gmail.com>",
  "version": "0.1.0",
  "description": "A universal localization managment tool for web projects",
  "bin": {
    "linguist": "lib/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "compile:watch": "babel -d lib/ src/ --watch",
    "prepublish": "npm run compile",
    "test": "npm run compile && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvkimball/linguist"
  },
  "keywords": [
    "i18n",
    "l10n",
    "Localization"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bvkimball/linguist/issues"
  },
  "homepage": "https://github.com/bvkimball/linguist#readme",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": [
      "linguist",
      "index"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "clear": "0.0.1",
    "commander": "^2.9.0",
    "crypsis": "latest",
    "fs-readline": "^2.1.0",
    "handlebars": "^4.0.5",
    "inquirer": "^0.12.0",
    "lodash": "4.3.0",
    "log-update": "^1.0.2",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.3.3",
    "passport": "^0.3.2",
    "rc": "^1.1.6",
    "rimraf": "^2.5.2",
    "yamljs": "^0.2.6"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^0.8.2",
    "jshint-summary": "^0.4.0"
  },
  "esformatter": {
    "indent": {
      "value": "    "
    },
    "plugins": [
      "esformatter-ignore"
    ]
  }
}
